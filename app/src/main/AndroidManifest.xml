<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.din.activitystart">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".lifecycle.LifeCycle"
            android:label="LifeCycle" />
        <activity
            android:name=".lifecycle.NormalActivity"
            android:label="NormalActivity" />
        <activity
            android:name=".lifecycle.DialogActivity"
            android:label="DialogActivity"
            android:theme="@android:style/Theme.Dialog" />

        <activity
            android:name=".singletop.TopActivity"
            android:label="TopActivity"
            android:launchMode="singleTop" />

        <activity
            android:name=".singleinstance.InstanceActivity"
            android:label="InstanceActivity"
            android:launchMode="singleInstance" />
        <activity
            android:name=".singleinstance.SecondActivity"
            android:label="SecondActivity" />
        <activity
            android:name=".singleinstance.ThirdActivity"
            android:label="ThirdActivity" />
        <activity
            android:name=".singleinstance.FourActivity"
            android:label="FourActivity" />

        <activity
            android:name=".singletask.TaskActivity"
            android:label="TaskActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".singletask.FourActivity"
            android:label="FourActivity" />
    </application>

</manifest>